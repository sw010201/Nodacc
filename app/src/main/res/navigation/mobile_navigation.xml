            app:destination="@id/nav_friend1" />
        <action
            android:id="@+id/action_nav_mycalendar_to_nav_friend3"
            app:destination="@id/nav_friend3" />
        <action
            android:id="@+id/action_nav_mycalendar_to_nav_friend4"
            app:destination="@id/nav_friend4" />
        <action
            android:id="@+id/action_nav_mycalendar_to_nav_friend2"
            app:destination="@id/nav_friend2" />
