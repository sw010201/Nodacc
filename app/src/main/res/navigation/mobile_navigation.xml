    <fragment
        android:id="@+id/nav_friend1"
        android:label="@string/friend1"
        tools:layout="@layout/fragment_friend1" />

    <fragment
        android:id="@+id/nav_friend2"
        android:label="@string/friend2"
        tools:layout="@layout/fragment_friend2" />

    <fragment
        android:id="@+id/nav_friend3"
        android:label="@string/friend3"
